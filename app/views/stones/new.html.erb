<%= simple_form_for @stone do |f| %>
  <%= f.input :name %>
  <%= f.input :category %>
  <%= f.input :age %>
  <%= f.input :weight %>
  <%= f.input :gender %>
  <%= f.input :price %>
  <%= f.input :address %>
  <%= f.input :zip %>
  <%= f.input :town %>
  <%= f.input :country %>
  <%= f.input :desccription %>
  <%= f.submit %>
<% end %>